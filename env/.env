# Logging level for application output
LOG_LEVEL=debug

# Controls whether Electron DevTools can be opened (true/false)
ENABLE_DEVTOOLS=true

# Application semantic version (kept in sync with package.json)
APP_VERSION=0.44.12

# Runtime target for the renderer stack (desktop | webapp)
APP_RUNTIME=desktop
VITE_APP_RUNTIME=desktop

# HTTP bridge defaults (used when running as a standalone webapp)
API_PROXY_TARGET=http://localhost:3333
VITE_API_BASE_URL=http://localhost:3333
